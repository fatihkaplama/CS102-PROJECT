<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/creativeMode_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background3"
    tools:context=".CreativeModePage">

    <Button
        android:id="@+id/settings_button_homePage2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/settings"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.962"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.021" />

    <Button
        android:id="@+id/volume_button_homePage2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/volumeon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.865"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.021" />

    <Button
        android:id="@+id/return_button_homePage2"
        android:layout_width="45dp"
        android:layout_height="44dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/back1"
        android:text="@string/return_button"
        app:layout_constraintEnd_toStartOf="@+id/voice_button_settingPage"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.13" />

    <LinearLayout
        android:id="@+id/creativeModeLinear_layout"
        android:layout_width="310dp"
        android:layout_height="267dp"
        android:background="@android:color/background_light"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="81dp"></LinearLayout>

    <LinearLayout
        android:layout_width="127dp"
        android:layout_height="242dp"
        android:background="@android:color/background_dark"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="579dp"
        tools:layout_editor_absoluteY="108dp">

        <Button
            android:id="@+id/button31"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:background="@drawable/buttonshape2"
            android:text="Button" />

        <Button
            android:id="@+id/button30"
            android:layout_width="match_parent"
            android:layout_height="27dp"
            android:background="@drawable/buttonshape2"
            android:text="Button" />

        <Button
            android:id="@+id/button29"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:background="@drawable/buttonshape2"
            android:text="Button" />

        <Button
            android:id="@+id/button25"
            android:layout_width="match_parent"
            android:layout_height="27dp"
            android:background="@drawable/buttonshape2"
            android:text="Button" />

        <Button
            android:id="@+id/button26"
            android:layout_width="match_parent"
            android:layout_height="22dp"
            android:background="@drawable/buttonshape2"
            android:text="Button" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/workingSpace_layout"
        android:layout_width="198dp"
        android:layout_height="243dp"
        android:background="@android:color/background_dark"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="373dp"
        tools:layout_editor_absoluteY="107dp"></LinearLayout>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="197dp"
        android:layout_height="26dp"
        android:background="@drawable/wallpaper6"
        android:gravity="center"
        android:text="@string/workingSpace_creativeMode"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="373dp"
        tools:layout_editor_absoluteY="80dp" />

    <TextView
        android:id="@+id/functions_layout"
        android:layout_width="126dp"
        android:layout_height="26dp"
        android:background="@drawable/wallpaper6"
        android:gravity="center"
        android:text="@string/functions_creativeMode"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="579dp"
        tools:layout_editor_absoluteY="80dp" />

</androidx.constraintlayout.widget.ConstraintLayout>